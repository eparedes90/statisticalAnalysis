plot(residuals(m1), ylab = "Residuals")
acf(residuals(m1), main = "ACF")
pacf(residuals(m1), main = "PACF")
dwtest(m1)
# Chunk 11: nor
#normalidad
jarque.bera.test(residuals(m1))
plot(density(residuals(m1)), main = "Residuals Density")
