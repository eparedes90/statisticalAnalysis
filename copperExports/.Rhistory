lmtest::bptest(m1)
glejser(
m1,
auxdesign = NA,
sigmaest = c("main", "auxiliary"),
statonly = FALSE
)
# Chunk 13: inestability
#chow test
sctest(m1,
type = "Chow")
#cusum test
cusum <- efp(volCobre_var ~ ., data =  data[, 8:12], type = "Rec-CUSUM")
plot(cusum)
View(data)
